{
    "commands": [
        {
            "command": "help",
            "args": "[-dm]",
            "example": "help -dm",
            "description": "Gives you information about the bot. If you use '-dm', it sends to you in DMs."
        },
        {
            "command": "ping",
            "args": "",
            "example": "",
            "description": "Gives you the bot's ping."
        },
        {
            "command": "channel",
            "args": "",
            "example": "",
            "description": "Set the channel to the guild's counting channel. Note: Commands will be disabled in the counting channel."
        },
        {
            "command": "channel none",
            "args": "",
            "example": "",
            "description": "Unlink the current counting channel."
        },
        {
            "command": "reset",
            "args": "",
            "example": "",
            "description": "Reset the count back to 0."
        },
        {
            "command": "toggle",
            "args": "[module]",
            "example": "toggle talking",
            "description": "Toggle modules. Leave empty to get a list of modules."
        },
        {
            "command": "subscribe",
            "args": "<count>",
            "example": "subscribe 1000",
            "description": "Subscribe to a count in the guild."
        },
        {
            "command": "topic",
            "args": "[-a] [topic]",
            "example": "topic Count to infinity!",
            "description": "Set the topic. Leave empty to clear topic. -a allows you to set the whole topic to your likings, including 'Next count:'. Use {{COUNT}} for the next count."
        },
        {
            "command": "set",
            "args": "<count>",
            "example": "set 1337",
            "description": "Set the count to a specific count."
        }
    ],
    "modules": [
        {
            "module": "allow-spam",
            "description": "This allows members to count more than once in a row, without someone else typing between.",
            "permissions": []
        },
        {
            "module": "talking",
            "description": "This allows members to talk after the count, ex. '1337 hello Admin!'",
            "permissions": []
        },
        {
            "module": "reposting",
            "description": "This makes the bot repost the message, preventing the user to edit or delete the count afterwards.",
            "permissions": []
        },
        {
            "module": "webhook",
            "description": "This makes the reposted message be a webhook. (Requires the 'reposting'-module)",
            "permissions": ["MANAGE_WEBHOOKS"]
        }
    ],
